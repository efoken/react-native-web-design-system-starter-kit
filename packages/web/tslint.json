{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-react-recommended",
    "tslint-react-native",
    "tslint-config-prettier"
  ],
  "rules": {
    "prettier": true,
    "promise-function-async": true,
    "newline-before-return": true,
    "no-implicit-dependencies": false
  },
  "rulesDirectory": ["tslint-plugin-prettier"]
}
